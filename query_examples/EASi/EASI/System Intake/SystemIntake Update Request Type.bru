meta {
  name: SystemIntake Update Request Type
  type: graphql
  seq: 18
}

post {
  url: {{url}}
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  mutation {
    updateSystemIntakeRequestType(id:"{{systemIntakeID}}", newType: MAJOR_CHANGES) {
      id
  
      requestType
  
      state
      decisionState
    }
  }
}

body:graphql:vars {
  {
      "id": "{{systemIntakeID}}"
  }
}

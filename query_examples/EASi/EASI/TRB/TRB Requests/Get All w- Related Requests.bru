meta {
  name: Get All w/ Related Requests
  type: graphql
  seq: 6
}

post {
  url: http://localhost:8080/api/graph/query
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  query trbRequests{
      trbRequests {
          id
          name
          relatedIntakes {
              id
              requestName
          }
          relatedTRBRequests {
              id
              name
          }
      }
  }
}

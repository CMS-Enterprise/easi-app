meta {
  name: TRBRequestAttendeesDelete
  type: graphql
  seq: 3
}

post {
  url: http://localhost:8080/api/graph/query
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  mutation {
    deleteTRBRequestAttendee(id:"{{trbAttendeeID}}") {
      id
      euaUserId
      trbRequestId
      component
      role
      createdBy
      createdAt
      modifiedBy
      modifiedAt
    }
  }
}

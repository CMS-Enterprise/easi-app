meta {
  name: Delete GRB Reviewer
  type: graphql
  seq: 6
}

post {
  url: {{url}}
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  mutation deleteGRBReviewer {
    deleteSystemIntakeGRBReviewer(input: {reviewerID: "{{SystemIntakeGRBReviewerID}}"})
  }
}

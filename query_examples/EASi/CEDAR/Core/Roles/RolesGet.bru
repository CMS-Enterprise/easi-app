meta {
  name: RolesGet
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  query Roles{
    roleTypes {
      id
      application
      name
      description
    }
  }
}

meta {
  name: Get Systems
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: apikey
}

auth:apikey {
  key: Authorization
  value: {{authHeader}}
  placement: header
}

body:graphql {
  query {
    cedarSystems{
      id
      name
      description
      acronym
      atoEffectiveDate
      atoExpirationDate
      status
      businessOwnerOrg
      businessOwnerOrgComp
      systemMaintainerOrg
      systemMaintainerOrgComp
      isBookmarked
    }
  }
}

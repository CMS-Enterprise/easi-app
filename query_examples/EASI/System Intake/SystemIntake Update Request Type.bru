meta {
  name: SystemIntake Update Request Type
  type: graphql
  seq: 19
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  mutation {
    updateSystemIntakeRequestType(id:"{{systemIntakeID}}", newType: MAJOR_CHANGES) {
      id
  
      requestType
  
      state
      decisionState
    }
  }
}

body:graphql:vars {
  {
      "id": "{{systemIntakeID}}"
  }
}

settings {
  encodeUrl: true
}

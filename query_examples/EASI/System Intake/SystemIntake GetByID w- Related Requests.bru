meta {
  name: SystemIntake GetByID w/ Related Requests
  type: graphql
  seq: 10
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query systemIntake {
      systemIntake(id: "{{systemIntakeID}}") {
          id
          requestName
          relatedIntakes {
              id
              requestName
          }
          relatedTRBRequests {
              id
              name
          }
      }
  }
}

settings {
  encodeUrl: true
}

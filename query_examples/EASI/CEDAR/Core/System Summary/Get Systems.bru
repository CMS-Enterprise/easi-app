meta {
  name: Get Systems
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query {
    cedarSystems{
      id
      name
      description
      acronym
      atoEffectiveDate
      atoExpirationDate
      status
      businessOwnerOrg
      businessOwnerOrgComp
      systemMaintainerOrg
      systemMaintainerOrgComp
      isBookmarked
    }
  }
}

settings {
  encodeUrl: true
}

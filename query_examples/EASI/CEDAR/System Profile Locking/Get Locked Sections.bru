meta {
  name: Get System Profile Section Locks
  type: graphql
  seq: 1
}

post {
  url: {{url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query systemProfileSectionLocks {
    systemProfileSectionLocks(cedarSystemId: "{{cedarSystemId}}") {
      cedarSystemId
      section
      isAdmin
      lockedByUserAccount {
        id
        username
        commonName
        email
      }
    }
  }
}

settings {
  encodeUrl: true
}


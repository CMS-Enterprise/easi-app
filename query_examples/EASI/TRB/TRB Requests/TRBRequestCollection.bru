meta {
  name: TRBRequestCollection
  type: graphql
  seq: 4
}

post {
  url: http://localhost:8080/api/graph/query
  body: graphql
  auth: inherit
}

body:graphql {
  query trbRequests{
   trbRequests(archived: false){
   id
   name
   archived
   type
   status
   relationType
   contractName
   contractNumbers {
   contractNumber
   }
   createdBy
   createdAt
   modifiedBy
   modifiedAt
   }
  }
}

settings {
  encodeUrl: true
}
